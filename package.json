    "systemjs":"^0.19.40",
    "tslint": "^3.15.1",
    "zone.js": "~0.6.23"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcvsling/angular2-vscode-debugable-prototype.git"
  }
